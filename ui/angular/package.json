{
  "name": "company-project-angular-ui",
  "version": "0.0.0",
  "scripts": {
    "start": "node --max_old_space_size=4192 node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --port 22305 -o",
    "hmr": "ng serve --host 0.0.0.0 --port 22305 -o --hmr",
    "buildng": "ng build --prod ",
    "build": "node --max_old_space_size=4192 node_modules/@angular/cli/bin/ng build --prod ",
    "staging": "node --max_old_space_size=4192 node_modules/@angular/cli/bin/ng build --configuration=staging",
    "e2e": "ng e2e",
    "analyze": "node --max_old_space_size=5120 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "ng lint --fix",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less --fix",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "color-less": "ng-alain-plugin-theme -t=colorLess",
    "theme": "ng-alain-plugin-theme -t=themeCss",
    "icon": "ng g ng-alain:plugin icon",
    "postinstall": "ngcc",
    "update-package": "yarn upgrade-interactive --latest"
  },
