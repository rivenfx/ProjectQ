{
  "Serilog": {
    "MinimumLevel": "Information",
    "Properties": {
      "ApplicationName": "ProjectQ Host"
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId",
      "ApplicationContext"
    ],
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"

    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt",
          "rollingInterval": "Hour"
        }
      }
    ]
  }
}